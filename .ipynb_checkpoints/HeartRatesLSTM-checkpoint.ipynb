    "def plot_heart_rate(ppg_data, index=0):\n",
    "    heart_rates = ppg_data.loc[index, '심박수_배열']\n",
    "    if pd.isna(heart_rates) or not isinstance(heart_rates, str):\n",
    "        print(f\"⚠️ Warning: index {index} has no valid heart rate data!\")\n",
    "        return\n",
    "    heart_rates = [x.strip() for x in heart_rates.split(',') if x.strip()]  \n",
    "\n",
    "    try:\n",
    "        heart_rates = list(map(float, heart_rates)) \n",
    "    except ValueError:\n",
    "        print(f\"⚠️ Warning: index {index} contains invalid data that cannot be converted to float!\")\n",
    "        return\n",
    "\n",
    "    plt.figure(figsize=(10, 5))\n",
    "    plt.plot(heart_rates, marker='o', linestyle='-', color='b', label=f'Heart Rate - {ppg_data.loc[index, \"심박측정일시\"]}')\n",
    "    plt.xlabel('Time (Index)')\n",
    "    plt.ylabel('Heart Rate (BPM)')\n",
    "    plt.title(f'Heart Rate Trend ({ppg_data.loc[index, \"심박측정일시\"]})')\n",
    "    plt.legend()\n",
    "    plt.grid(True)\n",
    "    plt.show()\n",
    "\n",
    "# 0번째 행의 심박수 그래프 그리기\n",
